local DarkraiX = loadstring(game:HttpGet("https://raw.githubusercontent.com/Fe773Vdoa/Hub/main/HubRoxa", true))()

local Library = DarkraiX:Window("Test","","",Enum.KeyCode.RightControl);

Tab1 = Library:Tab("Tab")

Tab1:Button("A Button",function()
    print("hi")
end)

Tab1:Dropdown("Dropdown",{"yo","sus","pro"},function(value)
            print(value)
end)

Tab1:Textbox("Textbox","",true,function(value)
print(value)
end)

Tab1:Seperator("Seperator")

Tab1:Line()

Tab1:Toggle("Toggle",false,function(value)
local plr = game.Players.LocalPlayer
local plrRP = plr.Character:FindFirstChild("HumanoidRootPart")
if plrRP then
  TweenSpeed = 200
  TweenPos = CFrame.new(-870.7681884765625, 193.59991455078125, -23.780122756958008)
  -- ///////////////////////////// --
  local distance = (plrRP.Position - TweenPos.p).Magnitude
  local duration = distance / TweenSpeed
  local tween = game:GetService("TweenService"):Create(plrRP, TweenInfo.new(duration), {CFrame = TweenPos})
  tween:Play()
  tween.Completed:Wait()
end
    end)

Tab1:Slider("Slider",1,100,25,function(value)
        print(value)
    end)

--[[
DarkraiX:ToggleUi()
]]
