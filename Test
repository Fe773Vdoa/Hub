local DarkraiX = loadstring(game:HttpGet("https://raw.githubusercontent.com/Fe773Vdoa/Hub/main/HubRoxa", true))()

local Library = DarkraiX:Window("Test","","",Enum.KeyCode.RightControl);

Tab1 = Library:Tab("Tab")

Tab1:Button("A Button",function()
    print("hi")
end)

Tab1:Dropdown("Dropdown",{"yo","sus","pro"},function(value)
            print(value)
end)

Tab1:Textbox("Textbox","",true,function(value)
print(value)
end)

Tab1:Seperator("Seperator")

Tab1:Line()

Tab1:Toggle("Toggle",false,function(value)
local function setBrightness()
    
    local lighting = game:GetService("Lighting")
    
    
    lighting.Ambient = Color3.new(1, 1, 1)
    
    
    lighting.OutdoorAmbient = Color3.new(1, 1, 1)
end

setBrightness()
end)

Tab1:Slider("Slider",1,100,16,function(value)
        local player = game.Players.LocalPlayer
local humanoid = player.Character.Humanoid

humanoid.WalkSpeed = value
    end)

Tab1:Label("Bem Vindo player.Name")

--[[
DarkraiX:ToggleUi()
]]
